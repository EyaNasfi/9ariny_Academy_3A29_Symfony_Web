 			<script>
				document.addEventListener("DOMContentLoaded", function () {
const formationItems = document.querySelectorAll('.formation-item');

// Object to keep track of whether a message has been shown for each item
const messageShown = {};

formationItems.forEach(item => {
item.addEventListener('mouseover', function () {
const course = item.getAttribute('data-course');
// Check if message has been shown for this item
if (! messageShown[course]) {
let message = '';
switch (course) {
case 'Encapsulation': message = 'Java is a powerful and versatile programming language used for a wide range of applications.';
break;
case 'polymorthisme': message = 'Explore mobile app development and create your own apps for iOS and Android platforms.';
break;
case 'c': message = 'C is a foundational programming language known for its efficiency and versatility, used in system programming and embedded systems.';
break;
case 'c++': message = 'C++ is an extension of the C programming language with added features for object-oriented programming and generic programming.';
break;
case 'bdd': message = 'Learn about Database Design and Development, covering topics like data modeling, normalization, and SQL.';
break;
case 'symfony': message = 'Symfony is a popular PHP framework for building web applications with a focus on speed, flexibility, and reusable components.';
break;
case 'javascript': message = 'JavaScript is a key programming language for web development, enabling dynamic and interactive user experiences.';
break;
case 'sql': message = 'SQL (Structured Query Language) is essential for managing and querying relational databases, used in a wide range of applications.';
break;
default: message = 'No specific message available for this course';
break;
}
// Display message only if it hasn't been shown before
if (message) {
Swal.fire({
title: 'Course Details',
html: message,
background: '#f3f3f3', // Change background color here
showCloseButton: true,
focusConfirm: false
});
// Set the flag to true to indicate that message has been shown for this item
messageShown[course] = true;
}
}
});
});
});
			</script>
